{
  "name": "cfi_card",
  "version": "1.0.0",
  "description": "Center Flight Information summary app",
  "private": true,
  "scripts": {
    "start1": "npx rimraf ./dist/ && concurrently \"npm:pm2\" \"npm:webpack\"",
    "start": "npm run pm2&&npm run webpack",
    "deploy": "npm install&&npm run build:production&&npm prune --production&&npm run production",
    "deploy:log": "npm install&&npm run build:p&&npm prune --production&&npm run production",
    "production": "pm2 start ./pm2/process.prod.config.js --env production",
    "build": "rimraf ./dist/ && webpack --mode development --progress --profile",
    "build:production": "rimraf ./dist/ && webpack --mode production --progress --profile --env.logger=logger",
    "build:p": "rimraf ./dist/ && webpack --mode production --env.logger=logger",
    "webpack": "rimraf ./dist/ &&webpack --mode development --watch",
    "webpack:stats": "npx rimraf ./dist/ && webpack --mode development --profile --json > webpack-stats.json",
    "devserver": "webpack-dev-server --mode development --devserver",
    "pm2": "pm2 start ./pm2/process.config.js",
    "migrate": "npx migrate-mongo -f src/migration/migrate.config.js",
    "lint": "npx eslint src/client/**/*.js src/client/**/*.jsx src/server/**/*.js",
    "lint:file": "npx eslint",
    "lint:fix": "npx eslint --fix",
    "lint:fix-all": "npx eslint --fix src/client/**/*.js src/client/**/*.jsx src/server/**/*.js"
  },
